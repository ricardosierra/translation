{
    "name": "ricardosierra/translation",
    "description": "An easy automatic database driven translator for Laravel 5.*",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "lang", "translation", "translate", "localization", "localize", "laravel"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/translation/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/ricardosierra/translation/issues",
        "source": "https://github.com/ricardosierra/translation",
        "docs": "https://github.com/ricardosierra/translation/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Sierra",
            "email": "ricardo@ricardosierra.com.br"
        },
        {
            "name": "Kunle Adekoya",
            "email": "crfystoline@gmail.com"
        },
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        },
        {
            "name": "Levan Velijanashvili",
            "email": "me@stichoza.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/validation": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "spatie/laravel-translatable": ">=4.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "*"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Translation\\": "src/",
            "Waavi\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Translation\\Tests\\": "tests/",
            "Waavi\\Translation\\Tests\\": "tests/"
        }
    },
    "replace": {
        "waavi/translation":"*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Translation\\TranslationServiceProvider"
            ],
            "aliases": {
              "Translation": "Translation\\Facades\\Translation"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
