{
    "name": "ricardosierra/translation",
    "description": "An easy automatic database driven translator for Laravel 5.*",
    "tags": ["lang", "translation", "translate", "localization", "localize", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Sierra",
            "email": "ricardo@ricardosierra.com.br"
        },
        {
            "name": "Kunle Adekoya",
            "email": "crfystoline@gmail.com"
        },
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.*",
        "illuminate/support": "5.*",
        "stichoza/google-translate-php": ">=4.0.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~4|~5|~6|~7"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "RicardoSierra\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RicardoSierra\\Translation\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "RicardoSierra\\Translation\\TranslationServiceProvider"
            ],
            "aliases": {
              "Translation": "RicardoSierra\\Translation\\Facades\\Translation"
            }
        }
    }
}
